# Your wallet address (required unless POSITION_ID is provided)
WALLET_ADDRESS=0x..

GRAPH_API_KEY=<GRAPH_API_KEY>

# Optional: Specific position NFT ID if you want to track a specific position
POSITION_ID=12345

# Schedule time in 24-hour format (HH:MM) (optional)
# Used only if you want to run the script via cron job
# This is otherwise set in .github/workflows/track-positions-and-deploy.yml
SCHEDULE_TIME=09:00

# Data file path
DATA_FILE_PATH=./data/positions.json

# Supabase
SUPABASE_URL=https://xxxx.supabase.co
SUPABASE_ANON_KEY=<SUPABASE_PUBLIC_ANON_KEY>