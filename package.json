{
  "name": "uniswap-position-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_NO_WARNINGS=1 node dist/index.js",
    "track": "NODE_NO_WARNINGS=1 tsx src/index.ts",
    "track:once": "NODE_NO_WARNINGS=1 tsx src/index.ts --once",
    "track:hourly": "NODE_NO_WARNINGS=1 tsx src/index.ts --hourly",
    "track:hourly:once": "NODE_NO_WARNINGS=1 tsx src/index.ts --once --hourly",
    "track-local": "NODE_NO_WARNINGS=1 SUPABASE_URL= SUPABASE_ANON_KEY= tsx src/index.ts",
    "track-local:once": "NODE_NO_WARNINGS=1 SUPABASE_URL= SUPABASE_ANON_KEY= tsx src/index.ts --once",
    "report": "NODE_NO_WARNINGS=1 tsx src/index.ts --report",
    "report-local": "NODE_NO_WARNINGS=1 SUPABASE_URL= SUPABASE_ANON_KEY= tsx src/index.ts --report",
    "prod:track:once": "NODE_NO_WARNINGS=1 node dist/index.js --once",
    "prod:track:hourly": "NODE_NO_WARNINGS=1 node dist/index.js --hourly",
    "prod:track:hourly:once": "NODE_NO_WARNINGS=1 node dist/index.js --once --hourly",
    "prod:report": "NODE_NO_WARNINGS=1 node dist/index.js --report",
    "telegram": "NODE_NO_WARNINGS=1 tsx src/notifyTelegram.ts",
    "test:telegram": "NODE_NO_WARNINGS=1 tsx src/notifyTelegram.ts --test",
    "prod:telegram": "NODE_NO_WARNINGS=1 node dist/notifyTelegram.js",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "@types/node-cron": "^3.0.11",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "node-cron": "^4.2.1"
  }
}
